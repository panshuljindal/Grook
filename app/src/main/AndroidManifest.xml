<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.panshul.grook">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MatchUp">
        <activity android:name=".Activity.ForgotPassword"></activity>
        <activity android:name=".Activity.CheckEmail" />
        <activity android:name=".Activity.EditProfile" />
        <activity android:name=".Activity.FinalCheckout" />
        <activity android:name=".Activity.GroundActivity" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.SignUp" />
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>